{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "KakeiBon",
"version": "1.1.1",
  "identifier": "org.zundou.kakeibon",
  "build": {
    "frontendDist": "./res",
    "beforeDevCommand": "",
    "beforeBuildCommand": ""
  },
  "app": {
    "windows": [
      {
        "title": "KakeiBon",
        "width": 1100,
        "height": 600,
        "minWidth": 1100,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
